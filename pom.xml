<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.panowow</groupId>
	<artifactId>wsimportdemo</artifactId>
	<version>0.0.1-SNAPSHOT</version>

	<build>
		<pluginManagement>
			<plugins>
				<!-- usage of jax-ws maven plugin -->
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>jaxws-maven-plugin</artifactId>
					<version>1.12</version>
					<executions>
						<execution>
							<id>wsimport-from-jdk</id>
							<goals>
								<goal>wsimport</goal>
							</goals>
							<configuration>
								<wsdlDirectory>src/wsdl</wsdlDirectory>
								<!-- which wsdl file -->
								<wsdlFiles>
									<wsdlFile>AgencyReportManagementService.wsdl</wsdlFile>
								</wsdlFiles>
								<!-- Keep generated files -->
								<keep>true</keep>
								<!-- Package name -->
								<packageName>agencyreportmanagement</packageName>
								<!-- generated source files destination -->
								<sourceDestDir>target/generatedclasses</sourceDestDir>
							</configuration>
						</execution>
						<execution>
							<id>wsimport-from-jdk2</id>
							<goals>
								<goal>wsimport</goal>
							</goals>
							<configuration>
								<wsdlDirectory>src/wsdl</wsdlDirectory>
								<!-- which wsdl file -->
								<wsdlFiles>
									<wsdlFile>ProcessorReportManagementService.wsdl</wsdlFile>
								</wsdlFiles>
								<!-- Keep generated files -->
								<keep>true</keep>
								<!-- Package name -->
								<packageName>processorreportmanagement</packageName>
								<!-- generated source files destination -->
								<sourceDestDir>target/generatedclasses</sourceDestDir>
							</configuration>
						</execution>
						<execution>
							<id>wsimport-from-jdk3</id>
							<goals>
								<goal>wsimport</goal>
							</goals>
							<configuration>
								<wsdlDirectory>src/wsdl</wsdlDirectory>
								<!-- which wsdl file -->
								<wsdlFiles>
									<wsdlFile>ReportManagementService.wsdll</wsdlFile>
								</wsdlFiles>
								<!-- Keep generated files -->
								<keep>true</keep>
								<!-- Package name -->
								<packageName>reportmanagementservice</packageName>
								<!-- generated source files destination -->
								<sourceDestDir>target/generatedclasses</sourceDestDir>
							</configuration>
						</execution>

					</executions>


				</plugin>
			</plugins>
		</pluginManagement>
	</build>


</project>